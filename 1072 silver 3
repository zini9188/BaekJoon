// 백준 1072 게임
// 이분탐색을 이용
#include <iostream>
using namespace std;
#define MAX 1000000000
long long int X, Y, Z;
int cnt;

int main() {
    cin >> X >> Y;
    Z = (Y * 100) / X;

    if (Z >= 99) {
        cout << "-1";
        return 0;
    }
    int left = 0, right = MAX;

    while (left <= right) {
        int mid = (left + right) / 2;
        int temp = (Y + mid) * 100 / (X + mid);
        if (Z < temp)
            right = mid - 1;
        else
            left = mid + 1;
    }
    cout << left;
}
