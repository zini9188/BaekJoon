// 백준 1541 잃어버린 괄호
#include <iostream>

using namespace std;
int main() {
    string input, temp;
    bool isSub = false;
    int sum = 0;

    //식을 입력받고 맨 앞에 +를 추가
    cin >> input;

    // input의 개수만큼 반복문을 처리.
    // -를 만나고 +로 변경을 안해도 되는 이유는
    // 괄호를 치게되면 전부 빼는게 최소값이 되기 때문에 빼기만 해도 된다.
    for (int i = 0; i <= input.length(); i++) {
        temp.clear();
        // 문자열에 숫자 추가
        while (i < input.length() && input[i] != '+' && input[i] != '-') {
            temp += input[i++];
        }

        if (isSub) {
            sum -= stoi(temp);
        } else {
            sum += stoi(temp);
        }

        if (!isSub && input[i] == '-')
            isSub = true;
    }
    cout << sum;
}
