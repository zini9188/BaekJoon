// 백준 1904 01타일
#include <iostream>
using namespace std;
// n = 1  --> 1
// n = 2  --> 00, 11
// n = 3  --> 001, 100 , 111
// n = 4  --> 0000, 0011, 1100, 1111, 1001
// n = 5  --> 00001, 00100, 10000, 11001, 10011, 00111, 11111, 11100
int arr[1000001] = {1, 2, 3};
int main() {
    for (int i = 3; i < 1000001; i++) {
        arr[i] = (arr[i - 2] + arr[i - 1]) % 15746;
    }
    int n;
    cin >> n;
    cout << arr[n - 1];
}
