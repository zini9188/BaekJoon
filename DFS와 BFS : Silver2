<https://www.acmicpc.net/problem/1260>

#include <iostream>
#include <vector>
#include <map>
#include <algorithm>
#include <queue>

using namespace std;
map<int, vector<int>> graph;
vector<bool> visited(1002, false);

void dfs(int node) { // 방문하지 않은 노드들을 순서대로 방문하며 출력
    visited[node] = true;
    cout << node << " ";
    for (int conn: graph[node]) {
        if (!visited[conn]) {
            visited[conn] = true;
            dfs(conn);
        }
    }
}

void bfs(int V) { // 방문하지 않은 노드들을 순서대로 큐에 넣고 팝하며 출력
    queue<int> q;
    q.push(V);
    visited[V] = true;
    while (!q.empty()) {
        int node = q.front();
        q.pop();
        cout << node << " ";

        for (int &nextNode: graph[node]) {
            if (!visited[nextNode]) {
                visited[nextNode] = true;
                q.push(nextNode);
            }
        }
    }
}

int main() {
    int N, M, V;
    cin >> N >> M >> V;
    int left, right;

    for (int i = 0; i < M; i++) { // 그래프 생성
        cin >> left >> right;
        graph[left].push_back(right);
        graph[right].push_back(left);
    }
    for (auto &a: graph) { // 각 노드들의 연결을 오름차순으로 정렬
        sort(a.second.begin(), a.second.end());
    }

    dfs(V);
    cout << endl;
    fill(visited.begin(), visited.end(), false);
    bfs(V);
}


