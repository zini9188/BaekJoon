// 백준 10844 쉬운 계단 수
#include <iostream>
#include <map>
using namespace std;
#define mod 1000000000
int dp[101][10] = {0};
int main() {
    int n;
    cin >> n;

    for (int i = 1; i <= 9; i++) {  // n = 1일 때 끝자리가 1~9일 때의 값
        dp[1][i] = 1;
    }

    for (int i = 2; i <= n; i++) {
        for (int j = 0; j <= 9; j++) {
            if (j == 0)  // 끝자리가 0이나 9이면 더할 수 있는 방향이 하나밖에 존재하지 않음
                dp[i][j] = dp[i - 1][j + 1] % mod;
            else if (j == 9)
                dp[i][j] = dp[i - 1][j - 1] % mod;
            else  // 나머지 상황에서 위 아래로의 값들을 더해줌
                dp[i][j] = (dp[i - 1][j - 1] + dp[i - 1][j + 1]) % mod;
        }
    }

    long long int ans = 0;
    for (int i = 0; i <= 9; i++)
        ans += dp[n][i];

    cout << ans % mod;
}
