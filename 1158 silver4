// 백준 1158 요세푸스 문제
#include <deque>
#include <iostream>
using namespace std;

int N, K;
string res = "<";
int main() {
    cin >> N >> K;

    deque<int> q;
    for (int i = 1; i <= N; i++)
        q.push_back(i);

    while (!q.empty()) {
        for (int i = 0; i < K - 1; i++) {
            q.push_back(q.front());
            q.pop_front();
        }
        res.append(to_string(q.front()) + ", ");
        q.pop_front();
    }
    res.erase(res.length() - 2, 2);
    res.append(">");
    cout << res;
}
