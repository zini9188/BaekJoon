#include <iostream>
#include <queue>
using namespace std;

int main() {
    queue<int> q;

    int n, len;
    cin >> n;
    for (int i = 1; i <= n; i++) {  // 1~n까지 q에 푸쉬
        q.push(i);
    }

    while (q.size() > 1) {  // q 사이즈가 0이 아니면 반복
        q.pop();            // 맨 처음 큐를 버림
        q.push(q.front());  // 그 다음 맨 앞의 q를 푸쉬
        q.pop();            // 방금 푸쉬한 것 팝
    }

    cout << q.front();
}
