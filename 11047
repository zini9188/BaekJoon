// 백준 11047 동전0

#include <iostream>
#include <stack>
using namespace std;
stack<int> s;
int n, k;
int main() {
    cin >> n >> k;
    int money;

    for (int i = 0; i < n; i++) {
        cin >> money;
        s.push(money);
    }
    int cnt = 0;
    while (k > 0 || !s.empty()) {
        if (k / s.top() > 0) {
            cnt += k / s.top();
            k %= s.top();
            s.pop();
        } else {
            s.pop();
        }
    }
    cout << cnt;
}
